<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.careerstart.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.student.ui.myprojects.MyProjectActivity"
            android:label="@string/title_activity_my_project"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ui.student.MainStudentActivity"
            android:label="@string/title_activity_main_student"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.company.MainCompanyActivity"
            android:label="@string/title_activity_projects"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>